      "source": [
        "n=int(input(\"To find out the prime numbers beginning with 1 in a list, enter the last number of the list you want :\"))\n",
        "liste=[]\n",
        "for i in range(1,n):\n",
        "  for x in range(2,i):\n",
        "    if (i%x==0):\n",
        "      break\n",
        "  else:\n",
        "    liste.append(i)\n",
        "print(liste)"
      ],
